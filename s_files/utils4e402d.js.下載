}
function formatAlbumnReadNum(e,n){
var t="en"===window.LANG,i=t?"k":"万",o="",r=1e4*n,a=t?10*n:n;
if(e=parseInt(e,10),e>r)o=a+i+"+";else if(e>=1e4&&r>=e){
var s=""+(t?e/1e3:e/1e4),l=s.indexOf(".");
o=-1===l?s+i:s.substr(0,l)+"."+s.charAt(l+1)+i;
}else o=e;
return o||0;
}
