}
function c(r,a){
var u;
switch(a){
case"UTF8":
case"UTF16BE":
case"UTF16LE":
break;

default:
throw Error("encoding must be UTF8, UTF16BE, or UTF16LE");
}
switch(r){
case"HEX":
u=t;
break;

case"TEXT":
u=function(r,t,n){
